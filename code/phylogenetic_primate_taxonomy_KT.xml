<?xml version="1.0"?>
<order clade="Primates">
    <groomingClaw>false</groomingClaw>
    <toothComb>false</toothComb>
    <fusedMandible>false</fusedMandible>
    <fusedFrontal>false</fusedFrontal>
    <postOrbitalClosure>false</postOrbitalClosure>
    <rhinarium>true</rhinarium>

    <semiOrder clade="Strepsirrhini">
        <groomingClaw>true</groomingClaw>
        <toothComb>true</toothComb>
        <rhinarium>true</rhinarium>
        <dentalFormula>2,1,3,3</dentalFormula>
        
        <location>Africa</location>
        <location>SouthEast Asia</location>
        
        <subOrder clade="Strepsirrhinis">
            <infraOrder clade="Lemuriformes">
                <location>Madagascar</location>
                <superFamily clade="Lemuroidea">
                	<family clade="Lemuridae">
                	</family>
                	
                	<family clade="Indriidae">
                	</family>
                	
                	<family clade="Cheirogaleidae">
                	</family>
                	
                	<family clade="Lepilemuridae">
                	</family>
                </superFamily>
                
                <superFamily clade="Daubentonoidea">
                	<family clade="Daubentoniidae">
                	</family>
                </superFamily>
            </infraOrder>
            
            <infraOrder clade="Lorisiformes">
                <location>Africa</location>
                <location>Asia</location>
                <superFamily clade="Lorisoidea">
                	<family clade="Lorisidae">
                	</family>
                	
                	<family clade="Galagidae">
                	</family>
                </superFamily>
            </infraOrder>
        </subOrder>
    </semiOrder>
    <semiOrder clade="Haplorhini">
        <fusedMandible>true</fusedMandible>
        <fusedFrontal>true</fusedFrontal>
        <postOrbitalClosure>true</postOrbitalClosure>
        <rhinarium>false</rhinarium>
        <dentalFormula>2,1,3,2</dentalFormula>
        
        <location>Africa</location>
        <location>Asia</location>
        <location>Americas</location>
        
        <subOrder clade="Tarsiiforme">
            <location>SouthEast Asia</location>
            <infraOrder clade="Tarsiiformes">
            	<superFamily clade="Tarsioidea">
            		<family clade="Tarsiidea">
            		</family>
            	</superFamily>
            </infraOrder>
        </subOrder>
        
        <subOrder clade="Anthropoidea">
            
            <infraOrder clade="Platyrrhini">
                <location>Americas</location>
                <superFamily clade="Pithecoidea">
                </superFamily>
                
                <superFamily clade="Ceboidea">
                </superFamily>
            </infraOrder>
            
            <infraOrder clade="Catarrhini">
                <location>Africa</location>
                <location>Asia</location>
                <superFamily clade="Cercopithecoidea">
                </superFamily>
                
                <superFamily clade="Hominoidea">
                </superFamily>
            </infraOrder>
        </subOrder>
    </semiOrder>
</order>